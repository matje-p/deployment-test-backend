{
    "version": 2,
    "builds": [
      {
        "src": "dist/routes/*.js",
        "use": "@vercel/node"
      },
      {
        "src": "dist/server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/dist/routes/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/dist/server.js"
      }
    ]
  }
  